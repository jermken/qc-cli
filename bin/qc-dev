#!/usr/bin/env node
const { spawn } = require('child_process')
const chalk = require('chalk')
const iconv = require('iconv-lite')
let runDev = spawn('vue-webpack-seed dev')
runDev.on('data', (data) => {
    console.log(chald.grey(data))
})
runDev.on('close', (code) => {
    console.log('子进程退出' + code)
})